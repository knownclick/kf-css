{
  "name": "kf-css",
  "version": "1.9.2",
  "description": "A modern, efficient CSS framework tailored for SvelteKit.",
  "main": "plugin/index.js",
  "types": "plugin/index.d.ts",
  "style": "dist/kf.css",
  "bin": {
    "kf-css": "./bin/kf-cli.js"
  },
  "exports": {
    ".": {
      "types": "./plugin/index.d.ts",
      "default": "./plugin/index.js"
    },
    "./vite": {
      "types": "./plugin/index.d.ts",
      "default": "./plugin/index.js"
    },
    "./sass": "./src/main.scss",
    "./css": "./dist/kf.css",
    "./min": "./dist/kf.min.css",
    "./responsive": "./dist/kf-responsive.css",
    "./ui": {
      "types": "./src/ui/index.d.ts",
      "default": "./src/ui/index.js"
    }
  },
  "scripts": {
    "build": "node bin/kf-cli.js build src/main.scss dist",
    "build:min": "sass src/main.scss dist/kf.min.css --style compressed",
    "prepublishOnly": "npm run build && npm run build:min"
  },
  "keywords": [
    "css",
    "sass",
    "sveltekit",
    "framework",
    "design-system"
  ],
  "author": "KF-CSS Contributors",
  "license": "MIT",
  "devDependencies": {
    "svelte": "^5.45.10",
    "vite": "^5.0.0"
  },
  "files": [
    "src",
    "dist",
    "bin",
    "plugin",
    "client.d.ts"
  ],
  "peerDependencies": {
    "svelte": "^4.0.0 || ^5.0.0",
    "vite": ">=5.0.0"
  },
  "dependencies": {
    "fs-extra": "^11.3.2",
    "prompts": "^2.4.2",
    "sass": "^1.81.0"
  }
}
